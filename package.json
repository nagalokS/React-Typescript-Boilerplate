{
  "private": true,
  "scripts": {
    "build": "cross-env ANAL=server webpack --config webpack.prod.js --progress",
    "build-start": "cross-env  webpack serve --config  webpack.prod.js --progress",
    "clean": "rm -rf build dist",
    "start": "cross-env   webpack serve --config webpack.dev.js --progress",
    "typeCheck": "tsc --noEmit",
    "format": "prettier --write \"./src\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write --parser markdown"
    ],
    "*.json": [
      "prettier --write --parser json"
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "media-query-plugin": "^1.5.0",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^7.5.0",
    "prettier": "^2.5.1",
    "react-refresh": "^0.10.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.1.0",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.50.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
